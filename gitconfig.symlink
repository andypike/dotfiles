[alias]
  conflicts = diff --name-only --diff-filter=U
[user]
  name = Andy Pike
  email = andy@andypike.com
[push]
  default = matching
[filter "hawser"]
  clean = git hawser clean %f
  smudge = git hawser smudge %f
  required = true
[core]
  editor = vim
[commit]
  template = ~/.gitmessage
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
